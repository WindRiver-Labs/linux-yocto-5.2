/dts-v1/;
#include "s32v234.dtsi"
/ {
	model = "Freescale S32V234";
	compatible = "fsl,s32v234-tmdp", "fsl,s32v234", "arm,vexpress,v2p-aarch64", "arm,vexpress";

	chosen {
		bootargs = "console=ttyLF0";
	};

};

&fec {
	status = "okay";
};
&i2c0 {
	status = "okay";
};
&i2c1 {
	status = "okay";
};
&i2c2 {
	status = "okay";
};
&swt0 {
	status = "okay";
};
&uart0 {
	status = "okay";
};
&usdhc0 {
	status = "okay";
};
